{
  "name": "@blog-starter/db",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./src/client.ts"
  },
  "scripts": {
    "migrate": "prisma migrate dev",
    "reset": "npx prisma migrate reset",
    "push": "prisma db push",
    "seed": "tsx src/seed.ts",
    "dev": "tsup src/* --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "lint": "eslint . --max-warnings 0",
    "studio": "prisma studio",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "reset:test": "dotenv -e .env.test -- npx prisma migrate reset",
    "reset:prod": "dotenv -e .env.production -- npx prisma migrate reset",
    "push:test": "dotenv -e .env.test -- prisma db push",
    "push:prod": "dotenv -e .env.production -- prisma db push",
    "seed:test": "dotenv -e .env.test -- tsx src/seed.ts",
    "seed:prod": "dotenv -e .env.production -- tsx src/seed.ts",
    "studio:test": "dotenv -e .env.test -- prisma studio",
    "studio:prod": "dotenv -e .env.production -- prisma studio"
  },
  "devDependencies": {
    "@blog-starter/eslint-config": "workspace:*",
    "@blog-starter/typescript-config": "workspace:*",
    "@prisma/client-runtime-utils": "^7.0.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "prisma": "^7.0.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1"
  }
}