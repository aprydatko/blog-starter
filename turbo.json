{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "LOG_LEVEL",
    "VITE_GIPHY_API_KEY"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "reset": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma",
        ".env*"
      ],
      "cache": false
    },
    "reset:prod": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma",
        ".env*"
      ],
      "cache": false
    },
    "reset:test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma",
        ".env*"
      ],
      "env": [
        "PRISMA_DDL_FORCE=true"
      ],
      "cache": false
    },
    "migrate": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "migrate:prod": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "migrate:test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": true
    },
    "push": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "push:prod": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "push:test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "seed": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": true
    },
    "seed:test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "seed:prod": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": true
    },
    "studio": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "packages/database/prisma/schema.prisma"
      ],
      "env": [
        "DATABASE_URL"
      ],
      "cache": false
    },
    "test": {
      "env": [
        "NEXTAUTH_URL",
        "AUTH_GITHUB_ID",
        "AUTH_GITHUB_SECRET",
        "AUTH_SECRET",
        "DATABASE_URL",
        "CI"
      ]
    },
    "test:e2e":{
      "cache": false
    },
    "test:integration":{
      "cache": false
    },
    "test:unit":{
      "cache": false
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false
    }
  }
}